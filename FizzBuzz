using System;

namespace FizzBuzz
{
    internal class Program
    {
        static void Main(string[] args)
        {

            int inputNumber = int.Parse(Console.ReadLine());
            string output = null;

            if (inputNumber % 2 == 0 && inputNumber % 3 == 0)
            {
                output = "FizzBuzz";
            }
            else if (inputNumber % 2 == 0)
            {
                output = "Fizz";
            }
            else if (inputNumber % 3 == 0)
            {
                output = "Buzz";
            }
            else
            {
                output = "OOPS!";
            }


            Console.WriteLine(output);

        }
    }
}
